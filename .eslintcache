it saver.post(data);\n\n  return result;\n};\n\n/**\n * Load user cat data from Storage.\n *\n * Require `await` keyword to call\n *   b/c this costs secs for data transfer.\n */\nexport const loadCatData = async () => {\n  const loader = new PseudoHandler(\"./data/load\");\n\n  const result = await loader.get();\n\n  return result;\n};\n\nexport default {\n  getCatImage,\n  getCatImagesWithKeyword,\n  saveCatData,\n  loadCatData\n};\n","/Users/yaguchitakuya/Repositories/react_your_fav_cat/src/api/data_handler.ts",[],["74","75"],"/Users/yaguchitakuya/Repositories/react_your_fav_cat/src/api/pseudo_handler.ts",[],{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":6,"column":8,"nodeType":"82","messageId":"83","endLine":6,"endColumn":13},{"ruleId":"80","severity":1,"message":"84","line":3,"column":19,"nodeType":"82","messageId":"83","endLine":3,"endColumn":23},{"ruleId":"80","severity":1,"message":"85","line":6,"column":8,"nodeType":"82","messageId":"83","endLine":6,"endColumn":16},{"ruleId":"80","severity":1,"message":"86","line":10,"column":10,"nodeType":"82","messageId":"83","endLine":10,"endColumn":40},{"ruleId":"80","severity":1,"message":"87","line":11,"column":10,"nodeType":"82","messageId":"83","endLine":11,"endColumn":25},{"ruleId":"88","severity":1,"message":"89","line":37,"column":3,"nodeType":"90","endLine":37,"endColumn":18},{"ruleId":"91","severity":1,"message":"92","line":63,"column":11,"nodeType":"90","endLine":63,"endColumn":57},{"ruleId":"88","severity":1,"message":"89","line":65,"column":13,"nodeType":"90","endLine":65,"endColumn":32},{"ruleId":"88","severity":1,"message":"89","line":68,"column":13,"nodeType":"90","endLine":68,"endColumn":58},{"ruleId":"88","severity":1,"message":"89","line":73,"column":17,"nodeType":"90","endLine":73,"endColumn":71},{"ruleId":"88","severity":1,"message":"89","line":90,"column":13,"nodeType":"90","endLine":90,"endColumn":62},{"ruleId":"88","severity":1